{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "rsl"
    },
    "environment": {
      "value": "prod"
    },
    "sqlAdminUsername": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/rsl-shared-resources/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "SqlAdminUsername"
      }
    },
    "sqlAdminPassword": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/rsl-shared-resources/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "SqlAdminPassword"
      }
    },
    "openAIApiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/rsl-shared-resources/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "OpenAIApiKey"
      }
    },
    "azureSearchEndpoint": {
      "value": "https://your-search-service.search.windows.net"
    },
    "azureSearchApiKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/rsl-shared-resources/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "AzureSearchApiKey"
      }
    },
    "jwtSecretKey": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/{subscription-id}/resourceGroups/rsl-shared-resources/providers/Microsoft.KeyVault/vaults/{kv-name}"
        },
        "secretName": "JwtSecretKey"
      }
    }
  }
}
