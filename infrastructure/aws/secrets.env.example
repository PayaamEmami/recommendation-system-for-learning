# RSL Secrets Configuration
# Copy this file to secrets.env and fill in the values
# NEVER commit secrets.env to git!

# Database username/password (create a strong password)
SQL_ADMIN_USERNAME=rsladmin
DB_PASSWORD=YourStrongPassword123!

# OpenAI API Key (get from https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-openai-key

# JWT Secret (64+ characters - generate with: openssl rand -base64 64)
JWT_SECRET=your-very-long-jwt-secret-key-at-least-64-characters-for-security

# OpenSearch endpoint (auto-populated after running deploy.sh)
OPENSEARCH_ENDPOINT=
