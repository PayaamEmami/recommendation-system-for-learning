{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_framework/*", "/css/*", "/js/*", "*.{css,js,json,ico,png,jpg,svg,woff,woff2,wasm}"]
  },
  "mimeTypes": {
    ".wasm": "application/wasm",
    ".dll": "application/octet-stream"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  },
  "routes": [
    {
      "route": "/_framework/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/css/*",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
